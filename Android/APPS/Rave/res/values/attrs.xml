<?xml version="1.0" encoding="utf-8"?>
<resources>
	<declare-styleable name="ProgressWheel">
        <attr name="text" format="string" />
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="barColor" format="color" />
        <attr name="rimColor" format="color" />
        <attr name="rimWidth" format="dimension" />
        <attr name="spinSpeed" format="dimension" />
        <attr name="delayMillis" format="integer" />
        <attr name="CircleColor" format="color" />
        <attr name="barWidth" format="dimension" />
        <attr name="barLength" format="dimension" />
        <attr name="contourColor" format="color"/>
        <attr name="contourSize" format="dimension"/>
    </declare-styleable>   
    
	<declare-styleable name="text">
        <attr name="invert" format="boolean" />
        <attr name="cap" format="boolean" />
    </declare-styleable>
    
	
    <declare-styleable name="CircularImage">
        <attr name="border__color" format="color" />
        <attr name="border_Width" format="integer" />
        <attr name="borderWidth" format="dimension" />
        <attr name="circular" format="boolean" />
        <attr name="image" format="integer" />
     </declare-styleable>
     
    <declare-styleable name="Tab">
        <attr name="left" format="boolean" />
     </declare-styleable>
     
    <declare-styleable name="ParallaxImageView">
        <attr name="intensity" format="float"/>
        <attr name="tiltSensitivity" format="float"/>
        <attr name="forwardTiltOffset" format="float"/>
        <attr name="scaledIntensity" format="boolean"/>
    </declare-styleable>
</resources>